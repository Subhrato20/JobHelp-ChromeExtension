{
    "name": "Word Highlighter",
    "version": "1.0",
    "description": "Highlights multiple user-specified words on the page.",
    "manifest_version": 3,
    "permissions": ["activeTab", "storage", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  